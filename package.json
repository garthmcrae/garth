{
  "name": "components",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "main": "dist/main.cjs.js",
  "module": "dist/main.esm.js",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@svgr/rollup": "4.2.0",
    "babel-eslint": "^10.0.1",
    "caniuse-lite": "^1.0.30001015",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "lint-staged": "8.1.6",
    "node-sass": "4.12.0",
    "prettier": "1.17.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "4.3.1",
    "react-scripts": "^3.3.0",
    "rollup": "1.11.3",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-url": "2.2.1",
    "stylelint": "10.0.1",
    "stylelint-config-recommended-scss": "3.3.0"
  },
  "peerDependencies": {
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-router-dom": "4.3.1"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "lint:js": "prettier --config ./.prettierrc --write \"src/**/*.js\" --loglevel silent && eslint \"src/**/*.js\" --quiet --fix",
    "lint:scss": "prettier --config ./.prettierrc --write \"src/**/*.scss\" --loglevel silent && stylelint \"src/**/*.scss\" --fix",
    "lint": "npm run lint:js && npm run lint:scss",
    "deploy": "npm run lint && npm run build && npm version patch && npm publish"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier --config ./.prettierrc --write --loglevel silent",
        "eslint --quiet --fix",
        "git add"
      ],
      "src/**/*.scss": [
        "prettier --config ./.prettierrc --write --loglevel silent",
        "stylelint --fix --cache --cache-location \".stylelintcache\"",
        "git add"
      ]
    }
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.4.0"
  }
}
